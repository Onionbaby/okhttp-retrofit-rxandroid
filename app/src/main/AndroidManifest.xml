<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="okhttp.demo.com.okhttpdemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".retrofitDemo.RetrofitActivity" />
        <activity android:name=".RxJavaDemo.RxJavaActivity" />
        <activity android:name=".KotlinDemo.KotlinActivity" />
        <activity android:name=".RecycleView.RecycleActivity" />
        <activity android:name=".RecycleView.KotlinRecycleActivity" />
        <activity android:name=".DebugDemo.DebugDemoActivity" />
        <activity android:name=".DebugDemo.URLListActivity" />
        <activity android:name=".TempActivity" />
        <activity android:name=".TouchDemo.TouchDemoActivity" />
        <activity android:name=".ScrollerDemo.ScrollerActivity" />
        <activity android:name=".TouchDemo.MultiTouchActivity" />
        <activity android:name=".TouchDemo.DragDropActivity" />
        <activity android:name=".TouchDemo.DragDropScrollActivity" />
        <activity android:name=".TouchDemo.ScaleActivity" />
        <activity android:name=".TouchDemo.TouchDemoListActivity"></activity>
    </application>

</manifest>
